<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="@dimen/onboard_indicator_inset"
        android:left="@dimen/onboard_indicator_inset"
        android:right="@dimen/onboard_indicator_inset"
        android:top="@dimen/onboard_indicator_inset">
        <shape android:shape="oval">
            <gradient
                android:angle="180"
                android:startColor="#4B5CBF"
                android:endColor="#8767DF"
                />
            <size
                android:width="@dimen/onboard_indicator_size"
                android:height="@dimen/onboard_indicator_size" />
        </shape>
    </item>
</layer-list>
